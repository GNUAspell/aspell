#DEFAULT Ä,\\"A,"A
#DEFAULT ä,\\"a,"a
#DEFAULT Ö,\\"O,"O
#DEFAULT ö,\\"o,"o
#DEFAULT Ü,\\"U,"U
#DEFAULT ü,\\"u,"u
#DEFAULT ß,\\"s,"s
#FLAGS utf-8
#ENDOPTION

#OPTION hyphen
#TYPE list
#DESCRIPTION characters used to encode hyphenation locations
#DEFAULT \\-
#ENDOPTION
