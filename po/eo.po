msgid "consider camel case words legal"
msgstr "konsideri MiksUsklajn vortojn validaj"

#: common/config.cpp:1463
msgid "save replacement pairs on save all"
msgstr "konservi anstataŭigajn parojn je 'konservi ĉion'"

